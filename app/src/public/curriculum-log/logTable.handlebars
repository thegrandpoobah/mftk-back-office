<table class="table table-striped">
  <thead>
    <tr>
      <th>Date</th>
      <th>Instructor</th>
      <th><abbr title="Poomsae Forms">PF</abbr></th>
      <th><abbr title="Appreciation Forms">AF</abbr></th>
      <th><abbr title="Self Defence">SD</abbr></th>
      <th><abbr title="Step Sparring">SS</abbr></th>
      <th style="width: 100%;">Additional Notes</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
  {{#.}}
    <tr>
      <td>{{createdAt}}</td>
      <td><a href="/admin/students/{{instructor.id}}/edit/" class="aviator">{{instructor.firstName}} {{instructor.lastName}}</a></td>
      <td>{{poomsaeForms}}</td>
      <td>{{appreciationForms}}</td>
      <td>{{selfDefense}}</td>
      <td>{{stepSparring}}</td>
      <td>{{extra}}</td>
    </tr>
  {{/.}}
  {{^.}}
    <tr>
      <td colspan="7">No lessons have been logged</td>
    </tr>
  {{/.}}
  </tbody>
</table>
