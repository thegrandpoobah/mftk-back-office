<div class="panel panel-default">
  <div class="panel-heading clearfix">
    Attendance Records
  </div>
  <table class="table">
    <thead>
      <tr>
        <th>Class</th>
        <th>Class Start Time</th>
        <th>Sign in Time</th>
        <th>Status</th>
      </tr>
    </thead>
    <tbody class="table-hover">
      {{#data}}
        <tr>
          <td>{{division.name}}</td>
          <td>{{division.startTime}}</td>
          <td>{{signInTime}}</td>
          <td>{{status}}</td>
        </tr>
      {{/data}}
      {{^data}}
        <td colspan="4">No attendance records found.</td>
      {{/data}}
    </tbody>
  </table>
</div>
